<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Profesor.ParticipantFrag">

    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:id="@+id/studentListItem"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:padding="5dp">

        <TextView
            android:id="@+id/participantListItem_nume"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Participant Nume"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:layout_weight="1"
            />
        <TextView
            android:id="@+id/participantListItem_email"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Participant Email"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:layout_weight="1"/>


    </LinearLayout>

    <ListView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/participantListView"
        android:layout_marginTop="30dp"
        />


</FrameLayout>
